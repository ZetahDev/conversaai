{
  "name": "conversaai-frontend",
  "type": "module",
  "version": "1.0.0",
  "description": "Frontend para ConversaAI - Plataforma de asistentes virtuales con IA",
  "scripts": {
    "dev": "astro dev --host 0.0.0.0 --port 4321",
    "start": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx,.astro",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx,.astro --fix",
    "format": "prettier --write .",
    "type-check": "astro check",
    "deploy:dev": "node scripts/deploy.js development",
    "deploy:staging": "node scripts/deploy.js staging",
    "deploy:prod": "node scripts/deploy.js production",
    "security:audit": "npm audit --audit-level=moderate",
    "security:fix": "npm audit fix",
    "clean": "rm -rf dist .astro node_modules/.cache",
    "analyze": "npm run build && npx astro build --analyze",
    "test": "echo \"No tests specified\" && exit 0"
  },
  "dependencies": {
    "@astrojs/node": "^8.3.4",
    "@astrojs/react": "^4.3.0",
    "@astrojs/tailwind": "^5.1.2",
    "astro": "^4.16.18",
    "date-fns": "^4.1.0",
    "immer": "^10.1.1",
    "lucide-react": "^0.525.0",
    "node-fetch": "^3.3.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "recharts": "^3.1.0",
    "tailwindcss": "^3.4.0",
    "zustand": "^5.0.6"
  },
  "devDependencies": {
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "typescript": "^5.4.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
